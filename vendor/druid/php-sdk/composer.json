{
  "name": "druid/php-sdk",
  "version": "1.1.1",
  "description": "DRUID SDK for PHP",
  "license": "Apache-2.0",
  "minimum-stability": "dev",
  "keywords": [
    "dru-id",
    "druid",
    "sdk",
    "php"
  ],
  "type": "library",
  "homepage": "https://dru-id.com/developers",
  "authors": [
    {
      "name": "dru-id",
      "homepage": "https://github.com/dru-id/druid-php-sdk/graphs/contributors"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "9.*"
  },
  "require": {
    "lib-curl": "*",
    "ext-dom": "*"
  },
  "config": {
    "vendor-dir": "src/main/php/lib/vendor"
  },
  "autoload": {
    "psr-4": {
      "Genetsis\\": "src",
      "Genetsis\\Tests\\" : "test/"
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "php -r \"mkdir('src/main/php/lib', 0777, true);\""
    ]
  }
}

